{
  "name": "soulprint-network",
  "version": "0.6.1",
  "description": "Soulprint validator node \u2014 HTTP server that verifies ZK proofs, co-signs SPTs, anti-Sybil registry",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "soulprint-node": "dist/server.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node scripts/compute-code-hash.mjs && cp -f src/blockchain/addresses.json dist/blockchain/addresses.json 2>/dev/null || true",
    "start": "node dist/server.js",
    "build:hash": "node scripts/compute-code-hash.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelariasfz/soulprint",
    "directory": "packages/network"
  },
  "homepage": "https://github.com/manuelariasfz/soulprint#readme",
  "keywords": [
    "soulprint",
    "validator-node",
    "kyc",
    "anti-sybil",
    "zero-knowledge",
    "decentralized"
  ],
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.16.0",
    "nodemailer": "^8.0.1",
    "otpauth": "^9.5.0",
    "otplib": "^13.3.0",
    "soulprint-core": "0.1.11",
    "soulprint-zkp": "0.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^7.0.11",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}